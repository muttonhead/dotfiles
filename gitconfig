[user]
	name = Corey Brothers
	email = corey.brothers@gmail.com
[core]
	editor = vim
	whitespace = cr-at-eol
[color]
	status = auto
	branch = auto
	diff = auto
	interactive = auto
[pager]
	diff = less
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=local --decorate
	unpushed = log --branches --not --remotes --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	undo = reset --hard HEAD~1
	st = status
	br = branch
	co = checkout
	cp = cherry-pick
	df = diff
	ci = commit
	a = add
	rb = rebase
  rbo = rebase origin/HEAD
	fo = fetch origin
	f = fetch --all
	rbc = rebase --continue
	rba = rebase --abort
  po = push origin HEAD

[includeIf "gitdir:~/work/neverfailcc/"]
	path = ~/work/neverfailcc/.gitconfig

[includeIf "gitdir:~/work/devsupply/"]
	path = ~/work/devsupply/.gitconfig
